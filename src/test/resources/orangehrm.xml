<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="OrangeHRM UI Automation Test" verbose="1" >
    <listeners>
        <listener class-name="core.MethodListener"></listener>
    </listeners>
    
    <parameter name="appUrl" value="http://localhost/orangehrm"></parameter>

    <test name="Run on FIREFOX">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="com.orangehrm.Level_12_Assert_Verify"></class>
        </classes>
    </test>
</suite>