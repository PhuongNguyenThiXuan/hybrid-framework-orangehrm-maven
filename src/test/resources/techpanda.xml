<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Techpanda UI Automation Test" verbose="1" >
    <listeners>
        <listener class-name="core.MethodListener"></listener>
    </listeners>
    
    <parameter name="appUrl" value="https://live.techpanda.org/"></parameter>

    <test name="Run on CHROME" preserve-order="true">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.techpanda.share.Register"></class>
<!--            <class name="com.techpanda.Level_20_Cookies"></class>-->
        </classes>
    </test>

</suite>